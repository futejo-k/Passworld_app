<Window x:Class="Passworld_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Passworld_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="1080" Width="1920">
    <Canvas>
        Background="#383838"
        <TextBox>
            <TextBox.Resources>
                <Style TargetType="TextBox">
                    <Setter Property="CornerRadius" Value="69"></Setter>
                </Style>
            </TextBox.Resources>
            Background="#626262"
            Width="990" Height="60"
            Canvas.Top="" Canvas.Left=""
        </TextBox>
        <TextBlock
            Foreground="#A0A0A0"
            Canvas.Top="211.19" Canvas.Left="115"
            Height="34.58" Width="256"
            FontSize="30"
            FontFamily="Space Grotesk"
        >Search password</TextBlock>
    </Canvas>
</Window>